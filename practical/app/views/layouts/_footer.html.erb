
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
<script>
  $(document).ready(()=>{
      setTimeout(()=>{
          $('.loader').hide();
      },1500);
  });
  $('#logout').on('click',()=>{
    setTimeout(()=>{
        location.href = "<%= root_path %>";
    },1000);

  });
</script>
